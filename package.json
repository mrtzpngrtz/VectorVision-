{
  "name": "vectorvision",
  "version": "0.9.0-alpha",
  "description": "High-performance desktop application for ML-powered image exploration and organization",
  "main": "electron-main.js",
  "license": "MIT",
  "author": "Moritz Pongratz",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrtzpngrtz/VectorVision.git"
  },
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.7.9",
    "electron": "^33.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "ml-som": "^0.0.6",
    "onnxruntime-node": "^1.20.1",
    "sharp": "^0.33.5",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "com.vectorvision.app",
    "productName": "VectorVision",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron-main.js",
      "preload.js",
      "public/**/*",
      "server/**/*",
      "models/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
