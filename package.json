{
  "name": "imagevector",
  "version": "1.0.0",
  "description": "Visual image exploration and organization tool",
  "main": "electron-main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev"
  },
  "dependencies": {
    "electron": "^28.0.0",
    "onnxruntime-node": "^1.16.3",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "ml-som": "^0.0.6",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.imagevector.app",
    "productName": "ImageVector",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron-main.js",
      "preload.js",
      "public/**/*",
      "server/**/*",
      "models/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
